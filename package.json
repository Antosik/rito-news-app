{
	"name": "rito-news-feeds-app",
	"version": "0.0.1",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"package": "svelte-kit package",
		"preview": "vite preview",
		"prepare": "svelte-kit sync",
		"lint": "npm run lint:svelte && npm run lint:ts && npm run lint:eslint",
		"lint:svelte": "svelte-check --tsconfig ./tsconfig.json",
		"lint:svelte:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint:ts": "tsc --noemit",
		"lint:eslint": "eslint \"src/**/*.{svelte,ts}\" --fix",
		"format": "prettier --write \"src/**/*.{svelte,ts,css,html}\""
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/kit": "next",
		"@types/leaflet": "^1.7.11",
		"@types/svelte-leafletjs": "^0.8.2",
		"@typescript-eslint/eslint-plugin": "^5.27.0",
		"@typescript-eslint/parser": "^5.27.0",
		"autoprefixer": "^10.4.7",
		"eslint": "^8.15.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-import-resolver-typescript": "^3.3.0",
		"eslint-plugin-import": "^2.26.0",
		"eslint-plugin-prettier": "^4.0.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"leaflet": "^1.8.0",
		"postcss": "^8.4.14",
		"postcss-load-config": "^4.0.1",
		"prettier": "^2.6.2",
		"prettier-plugin-svelte": "^2.7.0",
		"svelte": "^3.44.0",
		"svelte-check": "^2.7.1",
		"svelte-leafletjs": "^0.9.0",
		"svelte-multiselect": "^5.0.4",
		"svelte-preprocess": "^4.10.6",
		"tslib": "^2.3.1",
		"typescript": "^4.7.4",
		"vite": "^3.0.0",
		"vite-plugin-iso-import": "^0.1.3"
	},
	"type": "module",
	"dependencies": {
		"svelte-intl-precompile": "^0.11.1"
	}
}
