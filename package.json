{
	"name": "rito-news-feeds-app",
	"version": "0.0.1",
	"scripts": {
		"dev": "vite --port 3001",
		"build": "vite build",
		"build:dev": "vite build --mode development",
		"postinstall": "npm run kit:sync",
		"preview": "vite preview --port 3001",
		"lint": "concurrently -g \"npm:lint:svelte\" \"npm:lint:ts\" \"npm:lint:eslint\" \"npm:lint:css\"",
		"lint:fix": "concurrently -g \"npm:lint:svelte\" \"npm:lint:ts\" \"npm:lint:eslint:fix\" \"npm:lint:css:fix\"",
		"lint:svelte": "svelte-check --tsconfig ./tsconfig.json",
		"lint:css": "stylelint \"src/**/*.{svelte,css,scss}\"",
		"lint:css:fix": "stylelint \"src/**/*.{svelte,css,scss}\" --fix",
		"lint:ts": "tsc --noemit",
		"lint:eslint": "eslint \"src/**/*.{svelte,js,mjs,cjs,ts}\"",
		"lint:eslint:fix": "eslint \"src/**/*.{svelte,js,mjs,cjs,ts}\" --fix",
		"format": "prettier --write --plugin prettier-plugin-svelte \"src/**/*.{svelte,js,cjs,mjs,ts,css,scss,html,json}\"",
		"kit:sync": "svelte-kit sync"
	},
	"dependencies": {
		"feather-icons": "4.29.2",
		"leaflet": "1.9.4",
		"svelte-intl-precompile": "0.12.3",
		"svelte-leafletjs": "1.2.0",
		"svelte-multiselect": "10.3.0"
	},
	"devDependencies": {
		"@sveltejs/adapter-static": "3.0.6",
		"@sveltejs/vite-plugin-svelte": "3.1.2",
		"@sveltejs/kit": "2.7.7",
		"@total-typescript/ts-reset": "0.6.1",
		"@types/feather-icons": "4.29.4",
		"@types/leaflet": "1.9.14",
		"@types/node": "22.9.0",
		"@types/svelte-leafletjs": "0.8.7",
		"@typescript-eslint/eslint-plugin": "8.13.0",
		"@typescript-eslint/parser": "8.13.0",
		"autoprefixer": "10.4.20",
		"concurrently": "9.1.0",
		"eslint": "8.57.0",
		"eslint-config-prettier": "9.1.0",
		"eslint-import-resolver-typescript": "3.6.3",
		"eslint-plugin-import": "2.31.0",
		"eslint-plugin-prettier": "5.2.1",
		"eslint-plugin-svelte": "2.46.0",
		"postcss": "8.4.47",
		"postcss-load-config": "6.0.1",
		"prettier": "3.3.3",
		"prettier-plugin-svelte": "3.2.7",
		"sass": "1.80.6",
		"stylelint": "16.10.0",
		"stylelint-config-html": "1.1.0",
		"stylelint-config-idiomatic-order": "10.0.0",
		"stylelint-config-prettier-scss": "1.0.0",
		"stylelint-config-standard-scss": "13.1.0",
		"stylelint-order": "6.0.4",
		"stylelint-scss": "6.8.1",
		"svelte": "4.2.17",
		"svelte-check": "4.0.5",
		"svelte-preprocess": "6.0.3",
		"tslib": "2.8.1",
		"typescript": "5.6.3",
		"vite": "5.4.10",
		"vite-plugin-iso-import": "1.1.0"
	},
	"type": "module"
}
